  {
    "description": "enums in properties",
    "schema": {
      "type": "object",
      "properties": {"foo": {"enum": ["foo"]}, "bar": {"enum": ["bar"]}},
      "required": ["bar"]
    },
    "tests": [
      {
        "description": "both properties are valid",
        "data": {"foo": "foo", "bar": "bar"},
        "valid": true,
        "output": {
          "basic": {
            "valid": true,
            "keywordLocation": "#",
            "instanceLocation": "#"
          },
          "detailed": {
            "valid": true,
            "keywordLocation": "#",
            "instanceLocation": "#"
